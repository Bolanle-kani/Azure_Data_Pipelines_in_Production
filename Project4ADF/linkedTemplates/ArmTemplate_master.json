{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Project4ADF"
		},
		"ls_AzureDataLakeStorage_nyc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage_nyc'"
		},
		"ls_AzureSqlDatabase1_nyc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase1_nyc'"
		},
		"ls_AzureSynapseAnalytics_nyc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSynapseAnalytics_nyc'"
		},
		"ls_AzureDataLakeStorage_nyc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageacctnycproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Project4ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorage_nyc_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_nyc_accountKey')]"
					},
					"ls_AzureSqlDatabase1_nyc_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase1_nyc_connectionString')]"
					},
					"ls_AzureSynapseAnalytics_nyc_connectionString": {
						"value": "[parameters('ls_AzureSynapseAnalytics_nyc_connectionString')]"
					},
					"ls_AzureDataLakeStorage_nyc_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_nyc_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Project4ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Project4ADF_ArmTemplate_0')]"
			]
		}
	]
}